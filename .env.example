# Environment Variables for Jupiter DEX Substreams
# Copy this file to .env and fill in your values
# NEVER commit .env to the repository

# JWT Token for substreams.dev authentication
SUBSTREAMS_API_TOKEN=your-jwt-token-here

# Alternative JWT token variable
JWT_TOKEN=your-jwt-token-here

# Substreams endpoint (optional)
SUBSTREAMS_ENDPOINT=https://substreams.dev

# Log level (optional)
RUST_LOG=info